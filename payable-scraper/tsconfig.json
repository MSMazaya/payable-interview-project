{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "lib",
        "baseUrl": "./",
        "paths": {
            "engine/*": [
                "./engine/*"
            ],
            "scraper/*": [
                "./scraper/*"
            ],
            "utils/*": [
                "./utils/*"
            ],
            "types/*": [
                "./types/*"
            ]
        }
    },
    "lib": [
        "es2015"
    ]
}
